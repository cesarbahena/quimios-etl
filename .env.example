# LIMS ETL Environment Configuration
# Copy this file to .env and update with your actual values

# LIMS Server Configuration
# Production server (on-premises - internal network)
LIMS_BASE_URL=http://172.16.0.117
# For local testing with HTML fixtures, set to: true
LIMS_USE_LOCAL_FIXTURES=false

# Database Configuration
DB_USER=lims_user
DB_PASSWORD=lims_secure_pass
DB_NAME=lims_etl
DB_HOST=localhost
DB_PORT=5432

# LIMS Application Credentials
LIMS_USERNAME=your_lims_username
LIMS_PASSWORD=your_lims_password
LIMS_END_DATE=2021-01-15

# Cloud API Configuration (QuimiOSHub)
# Enable cloud synchronization
SYNC_TO_CLOUD=false
# Hub API endpoint (Oracle Cloud)
HUB_API_URL=https://your-hub-api.oraclecloud.com
# Optional API key for authentication
HUB_API_KEY=your_api_key_here
